<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Loan" parallel="tests" thread-count="5">
	
	<listeners>
		<listener class-name="Introduction.Listeners"/>
	</listeners>	
  <test thread-count="5" name="Personal">
	<parameter name="URL" value="https://rahulshettyacademy.com/"/>
    <classes>
      <class name="Introduction.Demo1"/>
      <class name="Introduction.Demo2"/>
    </classes>
  </test> <!-- Test -->
  
  <test thread-count="5" name="Car">
	<parameter name="URL" value="carloan.com"/>
    <classes>
      <class name="Introduction.Demo3">
		  <methods>
			  <exclude name='mobile.*'/>
		  </methods>
	  </class>
    </classes>
  </test> <!-- Test -->
  
   <test thread-count="5" name="Home">
	<parameter name="URL" value="homeloan.com"/>
	<parameter name="UserName" value="Kavya"/>
    <classes>
      <class name="Introduction.Demo4">
		  <methods>
			  <include name='APIhomeLoan()'/>
		  </methods>
	  </class>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
