package javacodes;

public class LearnStatic {
	
	String name;							//Instance variable(Global variable)
	String gender;
	String city;
	
	LearnStatic(String name, String gender, String city){		//Local variables
		this.name = name;
		this.gender = gender;
		this.city = city;
	}

	public static void main(String[] args) {
		
		LearnStatic ls = new LearnStatic("Monica","Female","Karur");
		ls.getAddress();
		LearnStatic ls1 = new LearnStatic("Navani","Male","Karur");
		ls1.getAddress();
		LearnStatic ls2 = new LearnStatic("Ragavi","Female","Karur");
		ls2.getAddress();
	}
	
	public void getAddress() {
		System.out.println(city);
	}

}
